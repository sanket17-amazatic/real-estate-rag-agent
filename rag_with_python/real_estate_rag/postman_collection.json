  },
  "item": [
    {
      "name": "Health & Stats",
      "item": [
        {
          "name": "Health Check",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "http://localhost:8000/health",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["health"]
            }
          }
        },
        {
          "name": "Get Stats",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "http://localhost:8000/stats",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["stats"]
            }
          }
        },
        {
          "name": "Root",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "http://localhost:8000/",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": [""]
            }
          }
        }
      ]
    },
    {
      "name": "PDF Ingestion",
      "item": [
        {
          "name": "Ingest PDF",
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": "/path/to/your/property.pdf"
                },
                {
                  "key": "locality",
                  "value": "Wakad",
                  "type": "text"
                },
                {
                  "key": "property_type",
                  "value": "Apartment",
                  "type": "text"
                }
              ]
            },
            "url": {
              "raw": "http://localhost:8000/ingest/pdf",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["ingest", "pdf"]
            }
          }
        }
      ]
    },
    {
      "name": "RAG Queries",
      "item": [
        {
          "name": "RAG Query - Amenities",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"What are the amenities in Wakad locality?\",\n  \"top_k\": 5,\n  \"include_context\": true\n}"
            },
            "url": {
              "raw": "http://localhost:8000/query/rag",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["query", "rag"]
            }
          }
        },
        {
          "name": "RAG Query - Project Details",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"Tell me about Evergreen Heights project\",\n  \"top_k\": 5,\n  \"include_context\": false\n}"
            },
            "url": {
              "raw": "http://localhost:8000/query/rag",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8000",
              "path": ["query", "rag"]
            }
          }
        }
      ]
    },
