# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Zilliz Cloud Configuration (Serverless)
# IMPORTANT: Use the Cluster Endpoint from Zilliz Cloud's "Connect" panel (NOT the console URL)
# Example: https://in03-xxxxx.api.gcp-us-west1.zillizcloud.com
MILVUS_URI=https://your-cluster-endpoint.api.region.zillizcloud.com

# Preferred auth for Zilliz Cloud: API key token (get from Zilliz Cloud console)
MILVUS_TOKEN=your_zilliz_api_key_here

# Optional: Traditional username/password auth (if not using token)
MILVUS_USER=db_admin
MILVUS_PASSWORD=your_password_here

# Database name
MILVUS_DB=default

# Collection name
MILVUS_COLLECTION=real_estate_properties

# Application Settings (Optional)
DEBUG=True
